            this.$store.dispatch('main')\n                .then(() => {\n                        this.todos = data.body\n                        console.log(\"DATA\",data)\n                })\n                .catch(err => console.log(err))\n        },\n      }\n    }\n\n\n</script>\n\n<style scoped>\n  .back-color{\n    color: #17799c;\n  }\n  .grey-color{\n    color: #666;\n  }\n\n</style>\n"]}]}